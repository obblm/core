{
    "name": "obblm/core",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Bundle to have Fantasy Bowl team management",
    "keywords": [],
    "homepage": "https://github.com/obblm/core",
    "authors": [
        {
            "name": "Benjamin Lanet",
            "homepage": "https://github.com/kumulo"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "intervention/image": "^2.5",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/password-hasher": "^5.4",
        "symfony/proxy-manager-bridge": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/uid": "^5.4",
        "symfony/validator": "^5.2",
        "symfony/yaml": "^5.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/php-code-coverage": "^9.2",
        "symfony/browser-kit": "^5.2",
        "phpunit/phpunit": "^9.5",
        "symfony/console": "^5.2",
        "symfony/css-selector": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Obblm\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obblm\\Core\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.x-dev"
        }
    }
}
