{
  "name": "obblm/core",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "Bundle to have Fantasy Bowl team management",
  "keywords": [],
  "homepage": "https://github.com/obblm/core",
  "authors": [
    {
      "name": "Benjamin Lanet",
      "homepage": "https://github.com/kumulo"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "intervention/image": "^2.5",
    "knplabs/knp-snappy-bundle": "^1.7",
    "sensio/framework-extra-bundle": "^5.5",
    "symfony/asset": "^4.4 || ^5.0",
    "symfony/console": "^4.4 || ^5.0",
    "symfony/dotenv": "^4.4 || ^5.0",
    "symfony/expression-language": "^4.4 || ^5.0",
    "symfony/flex": "^1.3.1",
    "symfony/form": "^4.4 || ^5.0",
    "symfony/framework-bundle": "^4.4 || ^5.0",
    "symfony/http-client": "^4.4 || ^5.0",
    "symfony/intl": "^4.4 || ^5.0",
    "symfony/mailer": "^4.4 || ^5.0",
    "symfony/messenger": "^5.0",
    "symfony/notifier": "^4.4 || ^5.0",
    "symfony/options-resolver": "^4.4 || ^5.0",
    "symfony/orm-pack": "*",
    "symfony/process": "^4.4 || ^5.0",
    "symfony/security-bundle": "^4.4 || ^5.0",
    "symfony/serializer-pack": "*",
    "symfony/string": "^4.4 || ^5.0",
    "symfony/translation": "^4.4 || ^5.0",
    "symfony/twig-pack": "*",
    "symfony/validator": "^4.4 || ^5.0",
    "symfony/web-link": "^4.4 || ^5.0",
    "symfony/yaml": "^4.4 || ^5.0",
    "twig/cssinliner-extra": "^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "phpunit/phpunit": "^8.5",
    "symfony/phpunit-bridge": "^4.4 || ^5.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": { "Obblm\\Core\\": "src/" },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Obblm\\Core\\Tests\\": "src/Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v1.x-dev"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
