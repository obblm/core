{
  "name": "obblm/core",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "Bundle to have Fantasy Bowl team management",
  "keywords": [],
  "homepage": "https://github.com/obblm/core",
  "authors": [
    {
      "name": "Benjamin Lanet",
      "homepage": "https://github.com/kumulo"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "intervention/image": "^2.5",
    "knplabs/knp-snappy-bundle": "^1.7",
    "sensio/framework-extra-bundle": "^5.5",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/intl": "*",
    "symfony/mailer": "*",
    "symfony/messenger": "5.1.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "*",
    "symfony/options-resolver": "*",
    "symfony/orm-pack": "*",
    "symfony/process": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer-pack": "*",
    "symfony/string": "*",
    "symfony/translation": "*",
    "symfony/twig-pack": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "twig/cssinliner-extra": "^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "symfony/phpunit-bridge": "^5.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": { "Obblm\\Core\\": "" },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Obblm\\Core\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v1.x-dev"
    }
  }
}
